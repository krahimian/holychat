@import '00_variables.styl'
@import 'nib'

[ng\cloak]
[ng-cloak]
.ng-cloak
    display none !important

.transition
    transition all 500ms cubic-bezier(0.10,0.70,0.10,1)

.scroll
    overflow-y scroll
    -webkit-overflow-scrolling touch

@media (max-width: $extraSmallMax)
    .hide-mobile
        display none